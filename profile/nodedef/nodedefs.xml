    <nodeDef id="WeatherFlow" nodeType="139" nls="ctl">
        <editors />
        <sts>
		<st id="ST" editor="bool" />
		<st id="GV0" editor="I_VOLTS" />
		<st id="GV1" editor="I_VOLTS" />
		<st id="GV2" editor="I_RSSI" />
		<st id="GV3" editor="I_RSSI" />
